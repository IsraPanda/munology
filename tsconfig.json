{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*",
    "src/content/config.ts"
  ],
  "exclude": ["dist", "node_modules"]
}
